<%= simple_form_for @video,:method => :post do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
  </div>
    <div>	
	<label for="video-url">URL</label>
	<br>
	<input id="video-url" type="text" name="video[url]">
  </div>

  <div class="field">
    <%= f.file_field :video, id: :video_video %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
  